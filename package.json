{
  "name": "stayallive-wp-sentry",
  "version": "3.0.0",
  "description": "A (unofficial) WordPress plugin to report PHP and JavaScript errors to Sentry",
  "repository": "https://github.com/CreativeFuse/cf.theme.fuse.git",
  "author": "Alex Bouma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stayallive/wp-sentry/issues"
  },
  "homepage": "https://github.com/stayallive/wp-sentry/",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.5.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "webpack": "^4.30.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-cli": "^3.3.2",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "@sentry/browser": "^5.1.2"
  },
  "scripts": {
    "dev": "cd ./assets/build && cross-env NODE_ENV=development webpack --mode development --watch --progress",
    "build": "cd ./assets/build && cross-env NODE_ENV=production webpack --mode production --progress"
  }
}
